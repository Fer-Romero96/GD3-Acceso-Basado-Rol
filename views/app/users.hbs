<div class="text-center mb-4">
    <h1 class="h3 mb-3 font-weight-normal">Usuarios</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                 <th scope="col">Password</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
                <tr>
                    <th scope="row">{{ id }}</th>
                    <td>{{ name }}</td>
                    <td>{{ email }}</td>
                    <td>{{ password }}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
    <button class="btn btn-lg btn-primary" onclick="window.location.href='/'">Home Page</button>
</div>